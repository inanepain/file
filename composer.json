{
    "name": "inanepain/file",
    "description": "File utilities for local and remote files.",
    "type": "library",
    "license": "UNLICENSE",
    "homepage": "http://vaieen.local:3000/inane/file",
    "readme": "README.md",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "peep@inane.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "peep@inane.co.za",
        "issues": "http://vaieen.local:3000/iane"
    },
    "keywords": [
        "inane",
        "file"
    ],
    "autoload": {
        "psr-4": {
            "Inane\\File\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "sort-packages": true
