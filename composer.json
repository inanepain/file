    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "inanepain/stdlib": ">=0.1.5 || dev-master || dev-develop",
        "inanepain/datetime": ">=0.3.1 || dev-master || dev-develop"
    },
    "require-dev": {}
}
